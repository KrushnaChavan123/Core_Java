// add,sub,mul,div,mode


class opt
{
 public static void main(String[] args)
 {
  int num1= 100, num2= 350, num3;
  num3 = num1 + num2;
  System.out.println(num3);
  num3 = num1-num2;
  System.out.println(num3);
  num3 = num1*num2;
  System.out.println(num3);
  num3 = num1/num2;
  System.out.println(num3);
  num3 = num1%num2;
  System.out.println(num3);
 }
}
 